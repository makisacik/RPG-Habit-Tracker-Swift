"starlight_hoop_description" = "Yıldız ışığı gibi parıldayan zarif bir küpe. Metal, ışıkta parlayan ve dans eden küçük yıldızları içeriyor gibi görünür.";

// Accessories - Lashes
"rose_blush" = "Gül Pembesi";
"rose_blush_description" = "Takan kişiye doğal, sağlıklı bir parlaklık veren hafif bir allık. Renk, güzelliği ve özgüveni artırır, kişiyi çekici kılar.";

// Gear - Head
"shadow_veil" = "Gölge Örtüsü";
"shadow_veil_description" = "Gölge özünden dokunmuş gizemli bir başlık. Kimliğini gizlerken temel koruma sağlar.";
"ironclad_crown" = "Demir Taç";
"ironclad_crown_description" = "En iyi demirden dövülmüş bu taç, görkemli görünümünü korurken mükemmel koruma sağlar. Birçok hevesli şövalye tarafından giyilmiştir.";
"crimson_crest" = "Kızıl Miğfer";
"crimson_crest_description" = "Kızıl taşlarla süslenmiş etkileyici bir miğfer. Metal içsel ateşle atıyor gibi görünür, takana korkutucu bir duruş kazandırır.";

// Gear - Outfits
"peasants_garb" = "Köylü Giysisi";
"peasants_garb_description" = "Köylüler tarafından giyilen basit ama dayanıklı giysiler. Alçakgönüllü görünümüne rağmen sayısız maceracının bilinmeyene ilk adımlarında hizmet etmiştir.";
"azure_tunic" = "Gökyüzü Tuniği";
"azure_tunic_description" = "Sabah çiyi gibi ışığı yakalayan güzel mavi bir tunik. Kumaş şaşırtıcı derecede dayanıklıdır, hem stil hem de koruma sağlar.";
"comfort_cloak" = "Rahat Pelerin";
"comfort_cloak_description" = "Nazik bir kucaklama gibi hissettiren sıcak ve rahat bir pelerin. Uzun çalışma geceleri veya sessiz düşünceler için mükemmeldir.";
"silken_grace" = "İpeksi Zarafet";
"silken_grace_description" = "En iyi ipekten dokunmuş zarif bir elbise. Kumaş su gibi akar ve detaylı işlemeler eski sarayların hikayelerini anlatır.";
"ironclad_plate" = "Demir Zırh";
"ironclad_plate_description" = "Otoriteyle gürültü çıkaran ağır demir zırh. Her plaka dikkatle işlenmiş ve uyarlanmıştır, fiziksel saldırılara karşı üstün koruma sağlar.";
"reinforced_ironclad" = "Güçlendirilmiş Demir Zırh";
"reinforced_ironclad_description" = "Ek takviyelerle güçlendirilmiş demir zırh. Metal eski tekniklerle işlenmiş, sıradan silahlara karşı neredeyse dayanılmaz hale getirilmiştir.";
"crimson_raiment" = "Kızıl Zırh";
"crimson_raiment_description" = "İçsel ateşle yanan cesur kırmızı zırh. Metal canlıymış gibi atar ve hem düşmanları korkutur hem de müttefikleri cesaretlendirir.";
"arcane_robes" = "Büyülü Cübbe";
"arcane_robes_description" = "Sihirli enerjilerle parıldayan gizemli cübbeler. Büyülü ipliklerle dokunmuş, büyü yeteneklerini artırır ve büyülü saldırılara karşı korur.";
"nightwing_shroud" = "Gece Kanadı Pelerini";
"nightwing_shroud_description" = "Işığı emiyormuş gibi görünen gizemli bir kıyafet. Gölgelerde yürüyenler tarafından giyilir ve takana doğaüstü bir varlık hissi verir.";
"inferno_mantle" = "Alev Pelerini";
"inferno_mantle_description" = "Saf ateş enerjisi yayan alevli bir kıyafet. Alevler kullanıcının etrafında dans eder ve yok edici bir güç aurası oluşturur.";

// Gear - Wings
"celestial_wings" = "Göksel Kanatlar";
"celestial_wings_description" = "İlahi ışıkla parlayan saf beyaz kanatlar. Saf enerjiden yapılmış gibidirler ve sahibine göklerde zarafetle uçma imkanı verirler.";
"phoenix_wings" = "Anka Kanatları";
"phoenix_wings_description" = "Ebedi ateşle yanan alevli kanatlar. Her tüy sıcaklıkla çıtırdar ve kanatlar yok oluşun küllerinden bile yeniden doğar.";
"blazing_phoenix" = "Alevli Anka";
"blazing_phoenix_description" = "Yoğunlaştırılmış ateşle yanan güçlendirilmiş anka kanatları. Isı o kadar güçlüdür ki yakın düşmanlar yeniden doğuşun yakıcı gücünü hisseder.";
"shadow_wings" = "Gölge Kanatlar";
"shadow_wings_description" = "Çevresindeki tüm ışığı emiyormuş gibi görünen kara kanatlar. Gece boyunca sessiz uçuş sağlar, görünmez kalmayı tercih edenler için mükemmeldir.";

// Gear - Weapons
"oakheart_blade" = "Meşe Kalpli Kılıç";
"oakheart_blade_description" = "Kadim meşeden oyulmuş bir kılıç, yıllar boyunca sertleşmiş bir bıçak. Basit olmasına rağmen doğal dengesi onu ustaların elinde etkili kılar.";
"copper_fang" = "Bakır Diş";
"copper_fang_description" = "Kendine has kırmızımsı tonu olan bir bakır kılıç. Metal, güneş ışığında parlayan keskin bir kenar oluşturmak için dikkatle dövülmüştür.";
"ironclad_edge" = "Demir Keskinlik";
"ironclad_edge_description" = "Sayısız savaşçının tercih ettiği güvenilir demir kılıç. Dengesi ve dayanıklılığı onu güvenilir bir savaş yoldaşı yapar.";
"steel_serpent" = "Çelik Yılan";
"steel_serpent_description" = "Saldıran bir yılan gibi hareket eden keskin çelik kılıç. Bıçak o kadar ince işlenmiştir ki daha zayıf zırhları kolayca kesebilir.";
"crimson_fang" = "Kızıl Diş";
"crimson_fang_description" = "İçsel ateşle yanan kızıl bir kılıç. Bıçak savaş için susamış gibi görünür, her vuruşta daha da ısınır.";
"blazing_fang" = "Alevli Diş";
"blazing_fang_description" = "Kızıl Diş'in geliştirilmiş hali, öyle yoğun yanar ki tek darbede düşmanları tutuşturabilir.";
"golden_dawn" = "Altın Şafak";
"golden_dawn_description" = "Şafak ışığını yakalayan efsanevi altın kılıç. Bıçağı sıcaklık ve umut yayar, müttefikleri cesaretlendirirken düşmanlara korku salar.";
"thunder_axe" = "Gök Gürültüsü Baltası";
"thunder_axe_description" = "Elektrik enerjisiyle çatırdayan ağır bir balta. Her sallayış gök gürültüsü gibi patlar ve bıçak yıldırımı yönlendirebilir.";
"storm_hatchet" = "Fırtına Balta";
"storm_hatchet_description" = "Fırtına enerjisiyle uğuldayan küçük bir balta. Büyük akrabalarından daha hafif olsa da yine de şimşek ve yıldırım gücünü taşıyabilir.";
"vipers_lash" = "Engerek Kırbacı";
"vipers_lash_description" = "Zehirli bir yılan gibi vuran esnek bir kırbaç. Deri özel yağlarla işlenmiş, onu hem ölümcül hem de sessiz yapmıştır.";
"arcane_staff" = "Büyülü Asa";
"arcane_staff_description" = "Büyü enerjisiyle atan sihirli bir asa. Kadim ağaçtan oyulmuş ve mistik kristallerle süslenmiştir, büyü yeteneklerini artırır ve güçlü büyüler kanalize eder.";
"iron_maiden" = "Demir Topuz";
"iron_maiden_description" = "Yıkıcı bir güçle vuran ağır bir topuz. Çivili başlığı zırhı ve kemikleri kolayca parçalayabilir, onu herkesin korkulu rüyası yapar.";
"soul_reaper" = "Ruh Biçen";
"soul_reaper_description" = "Kurbanlarının yaşamını emiyor gibi görünen ölümcül bir silah. Bıçak gece kadar karanlık ve ölüm kadar soğuktur, adını bilen herkes tarafından korkulur.";
"windseeker_bow" = "Rüzgar Arayıcı Yay";
"windseeker_bow_description" = "Rüzgarın kendisi tarafından yönlendiriliyormuş gibi görünen bir yay. Oklar olağanüstü bir doğrulukla uçar, en sert rüzgarlarda bile hedefini bulur.";
"stormcaller_bow" = "Fırtına Çağıran Yay";
"stormcaller_bow_description" = "Oklarıyla fırtınalar çağırabilen bir yay. Her atış yıldırımla çatırdar ve yay gök gürültüsünün gücüyle uğuldar.";

// Gear - Shields
"oakheart_ward" = "Meşe Kalpli Kalkan";
"oakheart_ward_description" = "Kadim meşeden oyulmuş, yüzeyi aşınmış ama güçlü bir kalkan. Sayısız darbeyi emmiş, her savaşçı için güvenilir bir savunma olmuştur.";
"crimson_ward" = "Kızıl Kalkan";
"crimson_ward_description" = "Savunma ateşiyle yanan kırmızı bir kalkan. Metal saldırıları emer ve ısı olarak geri yansıtır, düşmanları tekrar saldırmadan önce düşündürür.";
"ironclad_bulwark" = "Demir Siper";
"ironclad_bulwark_description" = "Zamanın testinden geçmiş sağlam bir demir kalkan. Yüzeyi savaş izleriyle doludur, her biri koruyucu gücünün kanıtıdır.";
"golden_aegis" = "Altın Aegis";
"golden_aegis_description" = "İlahi koruma yayan altın bir kalkan. Metal kutsal ışıkla parlar ve düşman saldırıları karşısında zayıflar.";

// Gear - Pets
"shadowpaw" = "Gölgepençe";
"shadowpaw_description" = "Koyu gece kadar siyah kürkü olan gizemli bir kedi yoldaş. Sessizce hareket eder ve düşüncelerini anlıyormuş gibi görünür, gizli görevler için mükemmel bir yoldaştır.";
"whiskerwind" = "Bıyıkrüzgar";
"whiskerwind_description" = "Yıldız ışığı gibi parıldayan kürkü olan zarif bir kedi. Tehlikeyi olağanüstü şekilde hisseder ve yaklaşan tehditler konusunda seni uyarır.";
"golden_cluck" = "Altın Gıdak";
"golden_cluck_description" = "Altın tüyleri güneş ışığında parlayan sadık bir tavuk yoldaş. Alçakgönüllü görünümüne rağmen altın gibi bir kalbe sahiptir ve seni her yere takip eder.";

// Collectibles
"crown_of_kings" = "Kralların Tacı";
"crown_of_kings_description" = "Tarih boyunca sayısız hükümdarın başını süslemiş görkemli bir taç. Mücevherleri geçmişin bilgeliğini taşır ve onu takmak otorite aurası kazandırır.";
"mystic_egg" = "Gizemli Yumurta";
"mystic_egg_description" = "Bilinmeyen bir enerjiyle atan gizemli bir yumurta. Kadim efsaneler bu yumurtalardan çıkan yaratıklardan bahseder ama kimse dönüşüme tanık olmamıştır.";
"thunder_hammer" = "Gök Gürültüsü Çekici";
"thunder_hammer_description" = "Elektrik enerjisiyle çatırdayan güçlü bir çekiç. Her darbe gök gürültüsü gibi patlar ve silah fırtınaların gücünü kanalize eder.";
"golden_key_of_ages" = "Çağların Altın Anahtarı";
"golden_key_of_ages_description" = "Sadece fiziksel kapıları değil daha fazlasını açıyor gibi görünen altın bir anahtar. Kadim yazıtlar, unutulmuş diyarların ve gizli bilgilerin kapılarını açabileceğini söyler.";
"silver_key_of_secrets" = "Sırlar Gümüş Anahtarı";
"silver_key_of_secrets_description" = "Onu tutanlara sırlar fısıldayan gümüş bir anahtar. Metal, açtığı her kilidi ve açığa çıkardığı her sırrı hatırlıyor gibi görünür.";
"medal_of_honor" = "Onur Madalyası";
"medal_of_honor_description" = "Büyük cesaret ve yiğitlik gösterenlere verilen prestijli bir madalya. Metal onur ışığıyla parlar ve takanı görenlerde saygı uyandırır.";
"harvest_pumpkin" = "Hasat Balkabağı";
"harvest_pumpkin_description" = "İçsel ışıkla parıldayan şenlikli bir balkabağı. Özenle oyulmuş ve mistik sembollerle süslenmiş, göründüğü her yere neşe getirir.";
"ring_of_power" = "Güç Yüzüğü";
"ring_of_power_description" = "Büyü enerjisiyle atan sihirli bir yüzük. Yüzeyine kadim runlar işlenmiştir ve onu takmak unutulmuş büyüsel yeteneklere erişim sağlar.";
"treasure_chest" = "Hazine Sandığı";
"treasure_chest_description" = "Tarih boyunca sayısız serveti saklamış sağlam bir hazine sandığı. Ahşabı aşınmış ama güçlüdür, kilit mekanizması eski mühendisliğin harikasıdır.";
"azure_treasure_chest" = "Gökyüzü Hazine Sandığı";
"azure_treasure_chest_description" = "İçsel ışıkla parıldayan nadir mavi bir hazine sandığı. Metali bilinen hiçbir alaşıma benzemez ve sandığın kendisi büyüsel enerjiyle uğuldar.";
"opened_treasure_chest" = "Açılmış Hazine Sandığı";
"opened_treasure_chest_description" = "Sırlarını çoktan açığa çıkarmış bir hazine sandığı. Şimdi boş olsa da hâlâ macera ve keşif vaadi taşır.";
"bronze_trophy_of_valor" = "Bronz Cesaret Kupası";
"bronze_trophy_of_valor_description" = "Cesaret ve yiğitlik için verilen bronz bir kupa. Metal parlatılmıştır ve işçilik büyük beceri ve özveriyi gösterir.";
"silver_trophy_of_glory" = "Gümüş Zafer Kupası";
"silver_trophy_of_glory_description" = "Zafer ışığıyla parıldayan gümüş bir kupa. Metal, geçmiş zaferlerin görkemini yakalar ve yansıtır.";
"golden_trophy_of_legends" = "Altın Efsane Kupası";
"golden_trophy_of_legends_description" = "Efsanenin gücüyle ışıldayan altın bir kupa. Metal, büyüklüğün özünü taşır ve ona sahip olanlar şöhrete yazgılıdır.";
"scroll_of_ancient_wisdom" = "Kadim Bilgelik Parşömeni";
"scroll_of_ancient_wisdom_description" = "Mistik runlarla kaplı kadim bir parşömen. Parşömen dikkatle incelendiğinde sırlar fısıldar.";
"scroll_of_lost_secrets" = "Kayıp Sırlar Parşömeni";
"scroll_of_lost_secrets_description" = "O kadar güçlü sırlar içeren bir parşömen ki yüzyıllarca saklanmıştır. Mürekkep yasak bilgiyle parlar.";
"scroll_of_forbidden_knowledge" = "Yasak Bilgi Parşömeni";
"scroll_of_forbidden_knowledge_description" = "Tehlikeli enerjiyle atan bir parşömen. İçindeki bilgi o kadar güçlüdür ki kadim bilginler dünyayı korumak için mühürlemiştir.";
"bell_of_awakening" = "Uyanış Çanı";
"bell_of_awakening_description" = "Ruhu uyandırıyormuş gibi çınlayan sihirli bir çan. Metal saf enerjiyle rezonansa girer ve çanı büyüleri bozabilir.";
"chronometer" = "Kronometre";
"chronometer_description" = "Saatlerden fazlasını ölçüyor gibi görünen mistik bir zaman ölçer. Dişlileri imkânsız hassasiyetle hareket eder ve zamanı ölçebilir.";
"lucky_four_leaf_clover" = "Şanslı Dört Yapraklı Yonca";
"lucky_four_leaf_clover_description" = "İyi şans yayan nadir bir dört yapraklı yonca. Yapraklar içsel ışıkla parlar ve onu taşımak sahibine şans getirir.";
"holy_cross" = "Kutsal Haç";
"holy_cross_description" = "İlahi ışıkla parlayan kutsal bir haç. Metal kutsanmış gibi görünür ve karanlık güçlere karşı koruma sağlar.";
"phoenix_feather" = "Anka Tüyü";
"phoenix_feather_description" = "Ebedi alevle yanan bir tüy. Ateş tarafından tüketilmesi gerekirken sağlam kalır ve sıcaklık yayar.";
"essence_of_fire" = "Ateş Özütü";
"essence_of_fire_description" = "Saf ateş enerjisi içeren bir kristal. Alevler içinde dans eder ve en soğuk kalpleri bile ısıtır.";
"lightning_bolt" = "Yıldırım Oku";
"lightning_bolt_description" = "Kristal formunda yakalanmış bir yıldırım. Enerji içinde çatırdar ve kristal fırtınaların gücüyle uğuldar.";
"strength_totem" = "Güç Totemi";
"strength_totem_description" = "Fiziksel güç enerjisi yayan bir totem. Güç sembolleriyle oyulmuş, takana cesaret verir.";
"starlight_fragment" = "Yıldız Işığı Parçası";
"starlight_fragment_description" = "Kristal formunda yakalanmış yıldız ışığı parçası. Uzak yıldızların ışığıyla parlar ve ilham getirir.";
"skull_of_the_fallen" = "Düşmüşlerin Kafatası";
"skull_of_the_fallen_description" = "Eski sahibinin anılarını taşıyor gibi görünen gizemli bir kafatası. Rahatsız edici olsa da geçmiş hakkında sırlar açığa çıkarabilir.";
"skull_of_the_ancient" = "Kadim Kafatası";
"skull_of_the_ancient_description" = "O kadar eski bir kafatası ki fosilleşmiş. Ondan kadim bilgelik yayılır.";

